{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "react/jsx-closing-bracket-location": [1, "line-aligned"],
    "react/jsx-closing-tag-location": 1,
    "react/jsx-curly-brace-presence": [1, {"children": "never", "props": "never"}],
    "react/jsx-curly-spacing": [1, {"attributes": true, "children": true, "when": "never"}],
    "react/jsx-equals-spacing": 1,
    "react/jsx-filename-extension": [2, {"extensions": [".js"]}],
    "react/jsx-first-prop-new-line": 1,
    "react/jsx-fragments": 1,
    "react/jsx-indent": [1, 2, {"checkAttributes": true, "indentLogicalExpressions": true}],
    "react/jsx-indent-props": [1, 2],
    "react/jsx-key": [1, {"checkFragmentShorthand": true}],
    "react/jsx-no-comment-textnodes": 1,
    "react/jsx-pascal-case": 1,
    "react/jsx-props-no-multi-spaces": 1,
    "react/jsx-sort-default-props": 1,
    "react/jsx-tag-spacing": [1, {"beforeClosing": "never"}],
    "react/jsx-uses-vars": 1,
    "react/no-children-prop": 0
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
